<div class="row">
<div class="span12">
  <div class="widget-block">
    <div class="widget-head">
      <h5>Especialistas Registrados</h5>
      <div class="widget-control pull-right">
        <a href="#" data-toggle="dropdown" class="btn dropdown-toggle"><i class="icon-cog"></i><b class="caret"></b></a>
        <ul class="dropdown-menu">
          <li id="add"><a href="#"><i class="color-icons add_co"></i> Registrar</a></li>
          <li id="edit"><a href="#"><i class="color-icons pencil_co"></i> Editar</a></li>
          <li id="delete"><a href="#"><i class="color-icons cross_co"></i> Eliminar</a></li>
        </ul>
      </div>
    </div>
    <div class="widget-content">
      <div class="widget-box">
      <!--comentarios aqui-->
        <table class="data-tbl-boxy table table-bordered">
        <thead>
        <tr>
          <th class="center" id="check"></th>
          <th>Cedula</th>
          <th>Nombres</th>
          <th>Apellidos</th>
          <th>Correo</th>
          <th>Telefono</th>
          <th>Especialidad</th>
        </tr>
        </thead>
       <tbody>
         <% @especialistas.each do |especialista| %>
           <tr>
              <td class="center tr-row-check"><input type="checkbox" name="checkbox12" id="row" value="<%= especialista.id %>"></td>            
              <td><%= especialista.cedula %></td>
              <td><%= especialista.nombres %></td>
              <td><%= especialista.apellidos %></td>
              <td><%= especialista.correo %></td>
              <td><%= especialista.telefono %></td>
              <td><%= especialista.especialidad.nombre %></td>
           </tr>
        <% end %>
      </tbody>
      </table>
    </div>
  </div>
  </div>
</div>
</div>
<div id="nuevo-dialog"></div>
<div id="messages"></div>
<div id="confirmacion"></div>