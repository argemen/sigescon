<script type="text/javascript">

  var gu;
$("#nuevo-dialog").dialog({ 
    autoOpen: false,
    title: 'Información del Registro',
    width: 400,
    resizable: false
 });

$("#messages").dialog({ 
    autoOpen: false,
    title: 'Operación no permitida',
    resizable: false,
    buttons: [{ text: "Ok", click: function() { $( this ).dialog( "close" ); } } ],
    show: {
        effect: "blind",
        duration: 50
      },
      hide: {
        effect: "explode",
        duration: 500
      }
 });


$("#confirmacion").dialog({ 
    autoOpen: false,
    title: 'Confirmación',
    buttons: {
        Aceptar: function() {
          $.ajax({
            type: "DELETE",
            url: "../<%= controller.controller_name %>/"+gu,
            data: {id: gu}
          })
            .done(function() {
              location.reload();
            })
            .fail(function() {
              location.reload();
            });
          $( this ).dialog( "close" );
        },
        Cancelar: function() {
          $( this ).dialog( "close" );
        }
      },
    show: {
        effect: "blind",
        duration: 50
      },
      hide: {
        effect: "explode",
        duration: 500
      }
 });

$("#add").click(function() {
    $("#nuevo-dialog").load("<%= controller.controller_name %>/new");
    $("#nuevo-dialog").dialog("open");
});

$("input").click(function(){
  gu = $("input:checked").val();
  
});

$("#edit").click(function() {
    if(gu==undefined) {
      $("#messages").html("Debe seleccionar un registro!!").dialog("open");
      return;
    }
    else{
      $("#nuevo-dialog").load("<%= controller.controller_name %>/" + gu + "/edit");
      $("#nuevo-dialog").dialog("open");

    }
});
$("#delete").click(function() {
  if(gu==undefined) {
      $("#messages").html("Debe seleccionar un registro!!").dialog("open");
      return;
    }
    else {
      $("#confirmacion").html("Esta seguro que desea eliminar el registro?").dialog("open");
    }
});
$(document).ready(function() {
  $(".lengthLabel").html('Registros por pagina');
});

$("#nuevo-reporte").dialog({ 
    autoOpen: false,
    title: 'Reportes',
    width: 600,
    resizable: false,
    show: {
        effect: "blind",
        duration: 50
      },
      hide: {
        effect: "explode",
        duration: 500
      }
 });
  $("#lista-docentes").click(function() {
      $("#nuevo-reporte").load("reportes/listaDocentes/");
      $("#nuevo-reporte").dialog("open");

    });
</script>