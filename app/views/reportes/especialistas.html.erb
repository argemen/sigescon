<div class="row">
<div class="span12">
  <div class="nonboxy-widget">
    <div class="widget-head">
      <h5>Especialistas Registrados</h5>
    </div>
    <div class="widget-content">
      <div class="widget-box">
      <!--comentarios aqui-->
        <table class="table table-default table-bordered">
        <thead>
        <tr>
          <th>#</th>
          <th>Cedula</th>
          <th>Nombres</th>
          <th>Apellidos</th>
          <th>Correo</th>
          <th>Telefono</th>
        </tr>
        </thead>
       <tbody>
         <% i = 1 %>
         <% @especialistas.each do |especialista| %>
           <tr>
              <td><%= i %></td>           
              <td><%= especialista.cedula %></td>
              <td><%= especialista.nombres %></td>
              <td><%= especialista.apellidos %></td>
              <td><%= especialista.correo %></td>
              <td><%= especialista.telefono %></td>
           </tr>
          <% i+=1 %>
        <% end %>
      </tbody>
      </table>
    </div>
  </div>
  </div>
</div>
</div>